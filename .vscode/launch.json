      "args": [
        "--input_mode=synthetic",
        "--rows=1000",
        "--cols=4096",
        "--num_trees",
        "1000",
        "--compute_oob_performances",
        "false"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
